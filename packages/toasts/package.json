{
  "name": "@tokene/toasts",
  "version": "0.0.0-rc.19",
  "description": "Vue based toasts",
  "repository": {
    "type": "git",
    "url": "https://github.com/dl-tokene/webkit.git",
    "directory": "packages/toasts"
  },
  "homepage": "https://dl-tokene.github.io/webkit/modules/_tokene_toasts.html",
  "license": "MIT",
  "sideEffects": false,
  "typesVersions": {
    ">=4.2": {
      "*": [
        "./dist/types/*"
      ]
    }
  },
  "main": "./dist/cjs/index.js",
  "module": "./dist/es/index.js",
  "browser": {
    "./sfc": "./dist/es/index.js"
  },
  "unpkg": "./dist/index.js",
  "types": "index.d.ts",
  "style": "./dist/index.css",
  "exports": {
    ".": {
      "types": "./dist/types/index.d.ts",
      "import": "./dist/es/index.js",
      "require": "./dist/cjs/index.js",
      "default": "./dist/index.js"
    },
    "./dist/index.css": {
      "default": "./dist/index.css"
    },
    "./*": "./*"
  },
  "files": [
    "dist",
    "es",
    "./web-types.json"
  ],
  "scripts": {
    "build": "yarn clean && vite build && yarn build:types && yarn build:web-types && node ./postbuild.js",
    "build:types": "vue-tsc -p tsconfig.build.json --outDir ./dist/types --declaration --emitDeclarationOnly",
    "build:web-types": "vue-docgen-web-types -c web-types-gen.config.js",
    "clean": "rm -rf dist",
    "test": "yarn jest --verbose --passWithNoTests",
    "typecheck": "tsc --noEmit"
  },
  "publishConfig": {
    "access": "public"
  },
  "dependencies": {
    "@distributedlab/w3p": "^0.2.0-rc.19",
    "@tokene/styles": "workspace:^",
    "@tokene/toolkit": "workspace:^",
    "@tokene/ui-kit": "workspace:^",
    "lodash": "^4.17.21",
    "vue-toastification": "^2.0.0-rc.5"
  },
  "peerDependencies": {
    "@vueuse/core": "^10.1.0",
    "vue": "^3.2.47"
  },
  "devDependencies": {
    "@rollup/plugin-alias": "^5.0.0",
    "@rollup/plugin-typescript": "^11.0.0",
    "@rushstack/eslint-patch": "^1.2.0",
    "@swc/cli": "^0.1.62",
    "@swc/core": "^1.3.53",
    "@swc/jest": "^0.2.26",
    "@types/jest": "^29.5.1",
    "@types/lodash": "^4.14.195",
    "@types/node": "^18.14.2",
    "@typescript-eslint/eslint-plugin": "^5.59.2",
    "@typescript-eslint/parser": "^5.59.2",
    "@vitejs/plugin-vue": "^4.2.1",
    "@vue/eslint-config-prettier": "^7.1.0",
    "@vue/eslint-config-typescript": "^11.0.3",
    "eslint-plugin-promise": "^6.1.1",
    "eslint-plugin-vue": "^9.11.0",
    "eslint-plugin-vue-i18n": "^0.3.0",
    "jest": "^29.5.0",
    "tsc-alias": "^1.8.2",
    "vite": "^4.3.3",
    "vite-svg-loader": "^4.0.0",
    "vue": "^3.2.47",
    "vue-docgen-web-types": "^0.1.8",
    "vue-router": "^4.1.6",
    "vue-tsc": "1.6.1"
  },
  "typedoc": {
    "entryPoint": "./typedoc.ts",
    "readmeFile": "./README.md",
    "displayName": "@tokene/toasts"
  },
  "web-types": "./web-types.json"
}
